{
    "name": "unixslayer/composer-replace",
    "description": "Shortcut for replacing old packages with new ones",
    "type": "composer-plugin",
    "require": {
        "php": "^7.3",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Unixslayer\\ComposerReplace\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "unixslayer",
            "email": "piotr.zajac@unixslayer.pl"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "class": "Unixslayer\\ComposerReplace\\ComposerReplace"
    }
}
